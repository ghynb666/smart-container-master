<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.fuguang.order.mapper.VerifyCodeMapper">
    <resultMap id="BaseResultMap" type="cn.fuguang.entity.VerifyCodeEntity">
        <result column="id" jdbcType="BIGINT" property="id"/>
        <result column="mobile" jdbcType="VARCHAR" property="mobile"/>
        <result column="verify_code" jdbcType="VARCHAR" property="verifyCode"/>
        <result column="order_no" jdbcType="VARCHAR" property="orderNo"/>
        <result column="retry_times" jdbcType="VARCHAR" property="retryTimes"/>
        <result column="invalid_time" jdbcType="DATETIME" property="invalidTime"/>
        <result column="create_time" jdbcType="DATETIME" property="createTime"/>
        <result column="update_time" jdbcType="DATETIME" property="updateTime"/>
    </resultMap>

    <insert id="insert" parameterType="cn.fuguang.entity.OrderInfoEntity">
        insert into order_info (mobile, verify_code, order_no,
                                retry_times , invalid_time, ,create_time, update_time)
        values (#{mobile,jdbcType=VARCHAR}, #{verifyCode,jdbcType=VARCHAR}, #{orderNo}, #{retryTimes},
                #{invalidTime,jdbcType=DATETIME},now(), now())
    </insert>
</mapper>
